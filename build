&& echo "(in .)" && cd . && make $1 && cd - > /dev/null \
&& echo "(in bmn)" && cd bmn && make $1 && cd - > /dev/null \
&& echo "(in examples/bmn2)" && cd examples/bmn2 && make $1 && cd - > /dev/null \

